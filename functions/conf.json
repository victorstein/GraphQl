{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["src/", "src/schema/", "src/helpers/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "logo": {
          "url": "https://upload.wikimedia.org/wikipedia/commons/1/17/GraphQL_Logo.svg",
          "width": "40px",
          "height": "40px",
          "link": "#"
        },
        "name": "Cartelera Nicaragua",
        "footerText": "Created by Alfonso Gomez",
        "css": [
            "./docStyles/styles.css"
        ]
    },
    "opts": {
        "encoding": "utf8",
        "destination": "../public/",
        "template": "node_modules/tui-jsdoc-template"
    }
}
